@article{clarke_quantum_1988,
	title = {Quantum Mechanics of a Macroscopic Variable: The Phase Difference of a Josephson Junction},
	volume = {239},
	issn = {0036-8075, 1095-9203},
	shorttitle = {Quantum Mechanics of a Macroscopic Variable},
	url = {http://www.sciencemag.org/content/239/4843/992},
