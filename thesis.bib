	year = {1994},
	keywords = {computational complexity, cryptosystems, discrete logarithms, factoring, Las Vegas algorithms, parallel algorithms, physical computational device, polynomial factor, quantum computation algorithms, quantum computer},
	pages = {124 --134}
},

