@article{yamamoto_quantum_2010,
	title = {Quantum process tomography of two-qubit {controlled-Z} and {controlled-NOT} gates using superconducting phase qubits},
	volume = {82},
	url = {http://link.aps.org/doi/10.1103/PhysRevB.82.184515},
	doi = {10.1103/PhysRevB.82.184515},
	abstract = {We experimentally demonstrate quantum process tomography of {controlled-Z} and {controlled-NOT} gates using capacitively coupled superconducting phase qubits. These gates are realized by using the {\textbar}2⟩ state of the phase qubit. We obtain a process fidelity of 0.70 for the controlled phase and 0.56 for the {controlled-NOT} gate, with the loss of fidelity mostly due to single-qubit decoherence. The {controlled-Z} gate is also used to demonstrate a two-qubit {Deutsch-Jozsa} algorithm with a single function query.},
