@article{galiautdinov_resonatorzero-qubit_2012,
	title = {Resonator–zero-qubit architecture for superconducting qubits},
	volume = {85},
	url = {http://link.aps.org/doi/10.1103/PhysRevA.85.042321},
	doi = {10.1103/PhysRevA.85.042321},
