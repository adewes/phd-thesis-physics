Cell[TextData[{
 "uS =U can be decomposed in the pauli basis opBase2: U = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{" ", 
      RowBox[{"m", "=", "1"}]}], "16"], 
    RowBox[{
     SubscriptBox["\[Alpha]", "m"], " ", 
     SubscriptBox["E", "m"]}]}], TraditionalForm]]],
 " =  ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["\[Alpha]", "\[RightVector]"], TraditionalForm]]],
 ". (we use decomposeInBase).\nAs ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Rho]", "out"], TraditionalForm]]],
 "= U",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    SubscriptBox["\[Rho]", "in"]}], TraditionalForm]]],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["U", "\[Dagger]"], TraditionalForm]]],
 " , we get  ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Rho]", "out"], TraditionalForm]]],
 "= ",
 Cell[BoxData[
  FormBox[
   UnderscriptBox["\[Sum]", 
    RowBox[{"m", ",", " ", "n"}]], TraditionalForm]]],
 " ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Alpha]", "m"], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["\[Alpha]", "n", "*"], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   SubscriptBox["E", "m"], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    SubscriptBox["\[Rho]", "in"]}], TraditionalForm]]],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    SubscriptBox["E", "m"], "\[Dagger]"], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   UnderscriptBox["\[Sum]", 
    RowBox[{"m", ",", " ", "n"}]], TraditionalForm]]],
 " ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Chi]", 
    RowBox[{"m", " ", "n"}]], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   SubscriptBox["E", "m"], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    SubscriptBox["\[Rho]", "in"]}], TraditionalForm]]],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    SubscriptBox["E", "m"], "\[Dagger]"], TraditionalForm]]],
 ". So \[Chi] = ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["\[Alpha]", "\[RightVector]"], TraditionalForm]]],
 " . ConjugateTranspose[ ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["\[Alpha]", "\[RightVector]"], TraditionalForm]]],
 " ]."
}], "Text",
 CellChangeTimes->{{3.5112429292221317`*^9, 3.511242950316017*^9}, {
